{
  "plugins": [
      "@babel/transform-async-to-generator",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      [
          "@babel/plugin-transform-runtime",
          {
              "corejs": 2
          }
      ]
  ],
  "presets": [
      "@babel/preset-react",
      [
          "@babel/preset-env",
          {
              "useBuiltIns": "entry"
          }
      ]
  ]
}
