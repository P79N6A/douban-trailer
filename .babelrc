{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-es2015"
  ],
  "plugins": [
    "@babel/transform-async-to-generator",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],

    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "transform-es2015-modules-commonjs",
    "@babel/plugin-proposal-export-default-from"
  ]
}
